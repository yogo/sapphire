%h1 Create New #{@collection.name} Item

%div{:class=>"form"}
  = form_for("item", :url => project_collection_items_path, :html => {"multipart" => true, "data-ajax"=> false}) do |f| 
    = render :partial => 'form', :locals => {:f => f, :collection => @collection}
    = f.submit("data-theme"=>"b")

  %a{:href => projects_path} Back

