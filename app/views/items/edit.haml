= form_for("item", @item, :url => project_collection_item_path(@project, @collection, @item), :html=>{:method=>"put", :class => 'form-box', :ajax=>false}) do |f| 
  = render :partial => 'items/form', :locals => {:f => f, :collection => @collection}
  .modal-footer
    %button.btn.btn-primary Save Changes
    %a.btn(data-dismiss="modal") Close

